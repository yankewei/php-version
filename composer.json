{
    "name": "yankewei/php-version",
    "description": "A better tool related to PHP version",
    "keywords": [
        "php-version",
        "php-version-checker",
        "php-version-compare"
    ],
    "homepage": "https://github.com/yankewei/php-version",
    "license": "MIT",
    "authors": [
        {
            "name": "yankewei",
            "email": "yankewei1993@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "phpstan/phpstan": "^2.1",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Yankewei\\PHP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yankewei\\PHP\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "phpstan": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
